#include <LiquidCrystal_I2C.h>
LiquidCrystal_I2C lcd_alex(32, 16, 2);

void setup()
{
  lcd_alex.init();
  
  lcd_alex.setCursor(0,0);
  lcd_alex.backlight(); 
}


void loop()
{
  lcd_alex.setCursor(0,0);
  lcd_alex.print("Alexander");
  lcd_alex.setCursor(0,1);
  lcd_alex.print("2020414");
}
